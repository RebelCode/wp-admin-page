{
    "name": "rebelcode/wp-admin-page",
    "description": "An object-oriented API for WordPress Admin pages.",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "RebelCode",
            "email": "dev@rebelcode.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^5.4 | ^7.0",
        "rebelcode/wp-admin-page-interface": "^0.1",
        "dhii/output-renderer-interface": "^0.1 | ^0.2 | ^0.3-alpha1",
        "dhii/invocable-interface": "^0.1-alpha1",
        "dhii/data-key-value-aware-abstract": "^0.1-alpha2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "dev-master",
        "codeclimate/php-test-reporter": "<=0.3.2",
        "dhii/stringable-interface": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "RebelCode\\WordPress\\Admin\\Page\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RebelCode\\WordPress\\Admin\\Page\\UnitTest\\": "test/unit",
            "RebelCode\\WordPress\\Admin\\Page\\FuncTest\\": "test/functional"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    }
}
